{
  "name": "rfai-contracts",
  "version": "0.2.4",
  "private": true,
  "description": "Includes RFAI contracts, migrations, tests",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "package-npm": "node scripts/package-npm.js",
    "deploy": "scripts/truffle-migrate",
    "compile": "node_modules/.bin/truffle compile",
